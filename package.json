{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.6.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "source-map-explorer": "^1.5.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.8",
    "@storybook/addon-links": "^4.0.0-alpha.8",
    "@storybook/addons": "^4.0.0-alpha.8",
    "@storybook/react": "^4.0.0-alpha.8",
    "babel-core": "^6.26.3",
    "babel-plugin-import": "^1.7.0",
    "babel-runtime": "^7.0.0-beta.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.4",
    "react-app-rewire-less": "^2.1.1",
    "react-app-rewired": "^1.5.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-app-rewired build",
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test": "react-app-rewired test --env=jsdom",
    "test-ci": "CI=true react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "precommit": "./script/hook/precommit.sh",
    "prepush": "yarn test-ci",
    "generateAction": "python script/util/generate-action.py",
    "generateContainer": "python script/util/generate-container.py"
  }
}
